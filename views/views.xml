<odoo>
  <data>
    <record model="ir.ui.view" id="odoo-producteca_sale_quote_tree">
      <field name="name">odoo-producteca.sale.quote.tree</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="sale.view_quotation_tree"></field>
      <field name="arch" type="xml">
        <tree>
		<field name="state" position="after">
			<field name="integration_id" />
		</field>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="odoo-producteca_sale_order_tree">
      <field name="name">odoo-producteca.sale.order.tree</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="sale.view_order_tree"></field>
      <field name="arch" type="xml">
        <tree>
		<field name="state" position="after">
			<field name="integration_id" />
		</field>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="producteca_account_invoice_tree">
      <field name="name">producteca.account.invoice.tree</field>
      <field name="model">account.invoice</field>
      <field name="inherit_id" ref="account.invoice_tree"></field>
      <field name="arch" type="xml">
        <tree>
		<field name="state" position="after">
			<field name="integration_id" />
		</field>
        </tree>
      </field>
    </record>

  </data>
</odoo>
